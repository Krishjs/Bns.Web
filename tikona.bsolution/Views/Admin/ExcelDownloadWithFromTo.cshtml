
@{
    Layout = null;
}

<!doctype html>
<html lang="en">
<head>
    <title>Admin</title>
    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
</head>
<body>
    <form action="/admin/ExcelDownloadWithFromTo" method="post">
        <div style="width:300px; margin:0 auto;margin-top: 25%;">
            <label for="tkname" style="float:left">Name:</label><input type="text" id="username" name="tkname" style="float:left;margin-left: 50px;margin-bottom: 14px;">
            <label for="tkpass" style="float:left">Password:</label><input type="password" id="password" name="tkpass" style="float:left;margin-left: 28px;margin-bottom: 14px;">
            <label for="from" style="float:left">From Date:</label><input type="date" id="from" name="from" style="float:left;margin-left: 20px;margin-bottom: 14px;width: 170px;">
            <label for="to" style="float:left">To Date:</label><input type="date" id="to" name="to" style="float:left;margin-left: 38px;margin-bottom: 14px;width: 170px;">
            <input type="submit" style="float:left;margin-left: 156px;margin-bottom: 14px;" value="Download Excel" />
        </div>
    </form>

</body>
</html>






